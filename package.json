{
  "name": "cf-worker-clocks",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "parcel src/pages/index.html",
    "dev-worker": "wrangler dev",
    "build": "rm -rf dist && next build",
    "start": "next start",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}'",
