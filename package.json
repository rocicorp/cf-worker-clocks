  },
  "dependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@types/node": "^17.0.16",
    "next": "^12.1.6",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "wrangler": "^2.8.0"
  }
}
